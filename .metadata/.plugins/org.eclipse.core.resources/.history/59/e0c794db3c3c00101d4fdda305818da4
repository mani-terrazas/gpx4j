package org.casaca.gpx4j.core.data;

public class Email {
	private String id;
	private String domain;
	
	public Email(String id, String domain) throws IllegalArgumentException{
		if(id == null ||Êdomain == null) throw new IllegalArgumentException("Id and domain must not be null");
		this.id = id;
		this.domain = domain;
	}
}
