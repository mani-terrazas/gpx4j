package org.casaca.gpx4j.core.data;

import java.util.ArrayList;
import java.util.List;

public class Route {
	private String name;
	private String cmt;
	private String desc;
	private String src;
	private Link link;
	private Integer number;
	private String type;
	private Extensions extensions;
	
	private List<Waypoint> waypoints;
	
	public Route(){
		this.name = null;
		this.cmt = null;
		this.desc = null;
		this.src = null;
		this.link = null;
		this.number = null;
		this.type = null;
		this.extensions = null;
		this.waypoints = new ArrayList<Waypoint>();
	}
}
